<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/trans_20px"
        android:background="@color/base_mask"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/trans_30px"
            android:paddingLeft="@dimen/trans_35px"
            android:paddingTop="@dimen/trans_30px"
            android:text=""
            android:textColor="@color/color_333333"
            android:textSize="@dimen/text_trans_28px"/>


        <com.fivefivelike.mybaselibrary.view.RoundButton
            android:id="@+id/tv_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/trans_30px"
            android:gravity="center"
            android:paddingBottom="@dimen/trans_10px"
            android:paddingLeft="@dimen/trans_25px"
            android:paddingRight="@dimen/trans_25px"
            android:paddingTop="@dimen/trans_10px"
            android:text="撤单"
            android:textColor="@color/mark_color"
            android:textSize="@dimen/text_trans_26px"
            app:btnCornerRadius="@dimen/trans_5px"
            app:btnStrokeColor="@color/mark_color"
            app:btnStrokeWidth="@dimen/trans_1px"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/trans_1px"
        android:layout_marginLeft="@dimen/trans_30px"
        android:layout_marginRight="@dimen/trans_30px"
        android:background="@color/base_mask"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/trans_10px"
        android:paddingTop="@dimen/trans_30px">

        <TextView
            android:id="@+id/tv_num_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="委托数量"
            android:textColor="@color/color_font3"
            android:textSize="@dimen/text_trans_22px"/>

        <TextView
            android:id="@+id/tv_transaction_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="已成交数量"
            android:textColor="@color/color_font3"
            android:textSize="@dimen/text_trans_22px"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/trans_35px">

        <TextView
            android:id="@+id/tv_num"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="--"
            android:textColor="@color/mark_color"
            android:textSize="@dimen/text_trans_30px"/>

        <TextView
            android:id="@+id/tv_transaction"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="--"
            android:textColor="@color/mark_color"
            android:textSize="@dimen/text_trans_30px"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/trans_1px"
        android:layout_marginLeft="@dimen/trans_30px"
        android:layout_marginRight="@dimen/trans_30px"
        android:background="@color/base_mask"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/trans_15px"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/trans_30px"
            android:layout_weight="1"
            android:text="委托价格"
            android:textColor="@color/color_font3"
            android:textSize="@dimen/text_trans_28px"/>

        <TextView
            android:id="@+id/tv_order_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/trans_30px"
            android:gravity="center"
            android:text="--"
            android:textColor="@color/color_font1"
            android:textSize="@dimen/text_trans_28px"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/trans_10px"
        android:layout_marginTop="@dimen/trans_5px"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/trans_30px"
            android:layout_weight="1"
            android:text="成交均价"
            android:textColor="@color/color_font3"
            android:textSize="@dimen/text_trans_28px"/>

        <TextView
            android:id="@+id/tv_travg_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/trans_30px"
            android:gravity="center"
            android:text="--"
            android:textColor="@color/color_font1"
            android:textSize="@dimen/text_trans_28px"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/trans_1px"
        android:layout_marginLeft="@dimen/trans_30px"
        android:layout_marginRight="@dimen/trans_30px"
        android:background="@color/base_mask"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/trans_20px"
        android:layout_marginTop="@dimen/trans_5px"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_statu"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/trans_35px"
            android:layout_weight="1"
            android:text=""
            android:textColor="@color/color_font1"
            android:textSize="@dimen/text_trans_30px"/>

        <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginRight="@dimen/trans_30px"
            android:text=""
            android:textColor="@color/color_font2"
            android:textSize="@dimen/text_trans_24px"/>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/trans_20px"
        android:background="@color/base_mask"
        />

</LinearLayout>